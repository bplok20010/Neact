{
  "name": "neact",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/neact.js neact.js.map neact.min.js lib/*",
    "minify": "uglifyjs dist/neact.js -c collapse_vars,evaluate,screw_ie8,unsafe,loops=false,keep_fargs=false,pure_getters,unused,dead_code -m -o dist/neact.min.js -p relative --in-source-map dist/neact.js.map --source-map dist/neact.min.js.map",
    "build:dist": "rollup -c",
    "build:cjs": "babel src -d lib",
    "build:es": "babel --no-babelrc src -d es",
    "build": "npm-run-all clean build:dist build:cjs build:es minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Nobo Zhou <nobo.zhou@foxmail.com>",
  "bugs": "https://github.com/bplok20010/neact/issues",
  "homepage": "https://github.com/bplok20010/neact",
  "repository": "https://github.com/bplok20010/neact",
  "files": [
    "*.md",
    "lib",
    "dist",
    "es",
    "index.js"
  ],
  "keywords": [
    "neact",
    "react",
    "react-dom",
    "virtual dom",
    "components",
    "vdom",
    "virtual",
    "jsx",
    "js"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-json": "^2.1.0",
    "uglify-js": "^2.7.5"
  }
}
